{
    "name": "@gavant/ember-table",
    "version": "1.0.4",
    "description": "Data tables built using ember-table",
    "keywords": [
        "ember-addon"
    ],
    "repository": "https://github.com/Gavant/gavant-ember-table",
    "license": "MIT",
    "author": "",
    "directories": {
        "doc": "doc",
        "test": "tests"
    },
    "scripts": {
        "build": "ember build --environment=production",
        "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
        "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
        "lint:hbs": "ember-template-lint .",
        "lint:hbs:fix": "ember-template-lint . --fix",
        "lint:js": "eslint . --cache",
        "lint:js:fix": "eslint . --fix",
        "start": "ember serve",
        "test": "npm-run-all tsc lint test:*",
        "test:ember": "ember test",
        "test:ember-compatibility": "ember try:each",
        "tsc": "tsc",
        "prepublishOnly": "ember ts:precompile",
        "postpublish": "ember ts:clean"
    },
    "dependencies": {
        "@ember/render-modifiers": "^1.0.2",
        "@glimmer/tracking": "^1.0.4",
        "ember-cli-babel": "^7.26.3",
        "ember-cli-htmlbars": "^5.7.1",
        "ember-cli-sass": "^10.0.1",
        "ember-cli-string-helpers": "^4.0.6",
        "ember-cli-typescript": "^4.1.0",
        "ember-did-resize-modifier": "^1.0.0",
        "ember-responsive": "^3.0.5",
        "ember-table": "3.0.2-2",
        "ember-truth-helpers": "^3.0.0",
        "ember-useragent": "0.9.1"
    },
    "devDependencies": {
        "@ember/optional-features": "^2.0.0",
        "@ember/test-helpers": "^2.2.5",
        "@embroider/test-setup": "^0.40.0",
        "@glimmer/component": "^1.0.4",
        "@types/ember": "^3.1.1",
        "@types/ember-qunit": "^3.4.7",
        "@types/ember__test-helpers": "^2.0.0",
        "@types/faker": "^5.5.4",
        "@types/qunit": "^2.9.0",
        "@types/rsvp": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "auto-changelog": "^2.2.1",
        "babel-eslint": "^10.1.0",
        "bootstrap": "^4.3.1",
        "broccoli-asset-rev": "^3.0.0",
        "ember-auto-import": "^1.11.2",
        "ember-bootstrap": "^4.6.3",
        "ember-cli": "~3.26.1",
        "ember-cli-dependency-checker": "^3.2.0",
        "ember-cli-github-pages": "^0.2.2",
        "ember-cli-inject-live-reload": "^2.0.2",
        "ember-cli-sri": "^2.1.1",
        "ember-cli-terser": "^4.0.1",
        "ember-cli-typescript-blueprints": "^3.0.0",
        "ember-disable-prototype-extensions": "^1.1.3",
        "ember-export-application-global": "^2.0.1",
        "ember-load-initializers": "^2.1.2",
        "ember-maybe-import-regenerator": "^0.1.6",
        "ember-page-title": "^6.2.1",
        "ember-qunit": "^5.1.4",
        "ember-resolver": "^8.0.2",
        "ember-source": "3.24.4",
        "ember-source-channel-url": "^3.0.0",
        "ember-template-lint": "^3.2.0",
        "ember-try": "^1.4.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-ember": "^10.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "expect-type": "^0.11.0",
        "faker": "^5.5.3",
        "loader.js": "^4.7.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.0",
        "qunit": "^2.14.1",
        "qunit-dom": "^1.6.0",
        "release-it": "^14.6.1",
        "sass": "^1.24.4",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": "10.* || >= 12"
    },
    "ember": {
        "edition": "octane"
    },
    "ember-addon": {
        "configPath": "tests/dummy/config",
        "after": [
            "ember-table"
        ]
    },
    "publishConfig": {
        "access": "public"
    }
}
